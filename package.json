{
  "name": "leafingio-node-seed",
  "version": "0.0.0-development",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 90 --branches 60 --functions 90 --lines 90",
    "report-coverage": "cat ./coverage/lcov.ingo | codecov",
    "test": "SERVER_ENVIRONMENT=test istanbul cover -x *.test.js _mocha -- -R nyan -r dotenv/config -r ./server/leafing/require --timeout 10000 server/**/*.test.js server/**/**/*.test.js",
    "dev": "SERVER_ENVIRONMENT=dev nodemon  -r dotenv/config -r ./server/leafing/require",
    "start": "SERVER_ENVIRONMENT=prod nodemon -r dotenv/config -r ./server/leafing/require ",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leafingio/node-seed.git"
  },
  "author": "leafingio",
  "license": "none",
  "dependencies": {
    "@risingstack/graffiti": "^3.2.0",
    "@risingstack/graffiti-mongoose": "^5.3.0",
    "async": "^2.1.4",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.1",
    "chalk": "^1.1.3",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "email-templates": "^2.5.4",
    "express": "^4.13.4",
    "express-graphql": "^0.6.1",
    "express-rate-limit": "^2.6.0",
    "express-validator": "^2.21.0",
    "file-stream-rotator": "0.0.7",
    "fs": "0.0.1-security",
    "graphql": "^0.8.2",
    "helmet": "^3.1.0",
    "ipware": "^1.0.0",
    "joi": "^10.0.1",
    "joi-objectid": "^2.0.0",
    "js2xmlparser": "^2.0.2",
    "jsonwebtoken": "^7.0.0",
    "mongoose": "^4.4.15",
    "mongoose-unique-validator": "^1.0.2",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "slug": "^0.9.1",
    "validator": "^5.7.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^1.3.2",
    "grunt": "^1.0.1",
    "grunt-apidoc": "^0.11.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-nsp": "^2.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "should": "^11.1.1",
    "supertest": "^2.0.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
